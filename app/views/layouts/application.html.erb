<!DOCTYPE html>
<html lang=ja>
  <head>
    <%= render 'shared/html_head'%>
  </head>

  <body>
  <%= render 'shared/header_main'%>
    <div class="container mt-5">
      <% flash.each do |message_type, message| %>
        <div class="alert alert-<%= message_type %> mb-3" id="alertsize"><%= message %></div>
      <% end %>
      <%= yield %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>
